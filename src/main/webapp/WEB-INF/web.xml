<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>GoodServlet</servlet-name>
    <servlet-class>com.qianfeng.fxmall.goods.controller.GoodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoodServlet</servlet-name>
    <url-pattern>/good.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GoodaddServlet</servlet-name>
    <servlet-class>com.qianfeng.fxmall.goods.controller.GoodAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoodaddServlet</servlet-name>
    <url-pattern>/goodAdd.do</url-pattern>
  </servlet-mapping>
</web-app>
